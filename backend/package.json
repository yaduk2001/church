{
    "name": "church-site-backend",
    "version": "1.0.0",
    "description": "Church Site Backend API - Node.js + Express + MongoDB",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc",
        "start": "ts-node src/server.ts",
        "start:prod": "node dist/server.js",
        "test": "jest",
        "test-connection": "ts-node src/test-connection.ts",
        "seed": "ts-node src/seed.ts",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "express",
        "mongodb",
        "typescript",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.41.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "exifr": "^7.1.3",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "fluent-ffmpeg": "^2.1.3",
        "googleapis": "^168.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.3",
        "mongodb": "^6.3.0",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-media-server": "^4.2.2",
        "socket.io": "^4.8.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.10.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}